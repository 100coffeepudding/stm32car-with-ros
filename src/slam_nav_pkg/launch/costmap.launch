<launch>
<node pkg="slam_nav_pkg" type="costmap_node" name="costmap_node" output="screen">
    <param name="inflation_radius_m" value="0.1"/>
    <param name="map_topic" value="/map"/>
    <param name="costmap_topic" value="/global_costmap"/>
    <param name="threshold" value="50"/>
    <param name="max_cost" value="100"/>
    <param name="min_cost" value="1"/>
</node>
    <node pkg="slam_nav_pkg" type="local_costmap_node" name="local_costmap_node" output="screen">
    <param name="robot_base_frame" value="base_link"/>
    <param name="map_frame" value="odom"/>
    <param name="radius_m" value="2.0"/>
    <param name="resolution" value="0.05"/>
    <param name="inflation_radius" value="0.15"/>

</node>

</launch>